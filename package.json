{
  "name": "org-comment",
  "version": "1.0.0",
  "description": "API Service to Record Comment For Available Github Organization",
  "main": "build/index.js",
  "scripts": {
    "start": "tsc && node build/index.js",
    "dev": "node ./node_modules/nodemon/bin/nodemon.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint src/** --ext .ts,.tsx",
    "lint:fix": "npm run lint --fix",
    "git:commit": "git add . && git commit -m",
    "git:push": "git push --all",
    "migrate": "./node_modules/.bin/node-pg-migrate"
  },
  "keywords": [
    "coding",
    "node",
    "js"
  ],
  "author": "Almas Hilman Muhtadi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/lmnzr/organization-comment"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run --silent lint"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run --silent lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "connection-string-parser": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.20.1",
    "jsonwebtoken": "^8.5.1",
    "node-pg-migrate": "^3.22.0",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/uuid": "^3.4.5",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/helmet": "0.0.44",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/express": "^4.17.1",
    "@types/pg": "^6.1.47",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "nodemon": "^1.19.1",
    "eslint": "^5.16.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "typescript": "^3.6.2"
  }
}
